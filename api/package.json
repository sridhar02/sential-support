{
  "name": "sentinel-support-api",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma": "prisma",
    "eval": "ts-node --project tsconfig.scripts.json scripts/runEvals.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@prisma/client": "5.15.0",
    "cors": "2.8.5",
    "csv-parse": "5.5.3",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-async-errors": "3.1.1",
    "helmet": "7.1.0",
    "ioredis": "5.4.1",
    "pino": "9.1.0",
    "prom-client": "15.1.3",
    "uuid": "9.0.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.12.8",
    "@types/uuid": "9.0.7",
    "pino-pretty": "11.0.0",
    "prisma": "5.15.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  },
  "prisma": {
    "seed": "ts-node --project ./tsconfig.scripts.json prisma/seed.ts"
  }
}
